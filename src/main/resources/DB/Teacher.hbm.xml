<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DB_model.Teacher" table="teacher" schema="socialpractice">
        <composite-id mapped="true" class="DB_model.TeacherPK">
            <key-property name="teacherNum" column="teacherNum"/>
            <key-property name="leaderStuNum" column="leaderStuNum"/>
        </composite-id>
        <property name="name" column="name"/>
        <property name="jobTitle" column="jobTitle"/>
        <property name="department" column="department"/>
        <property name="phoneNum" column="phoneNum"/>
        <property name="email" column="email"/>
        <property name="idType" column="idType"/>
        <property name="idNum" column="idNum"/>
        <property name="birthday" column="birthday"/>
        <property name="expertFlag" column="expertFlag"/>
    </class>
</hibernate-mapping>