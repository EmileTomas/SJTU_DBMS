<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DB_model.Team" table="team" schema="socialpractice">
        <id name="leaderStuNum" column="leaderStuNum"/>
        <property name="projectName" column="projectName"/>
        <property name="projectType" column="projectType"/>
        <property name="projectIndex" column="projectIndex"/>
        <property name="importantFlag" column="importantFlag"/>
        <property name="practiceLocation" column="practiceLocation"/>
        <property name="startTime" column="startTime"/>
        <property name="endTime" column="endTime"/>
        <property name="teacherNum" column="teacherNum"/>
        <property name="expertNum" column="expertNum"/>
        <property name="applyStatus" column="applyStatus"/>
    </class>
</hibernate-mapping>