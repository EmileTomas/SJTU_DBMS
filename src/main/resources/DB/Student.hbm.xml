<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DB_model.Student" table="student" schema="socialpractice">
        <id name="stuNum" column="stuNum"/>
        <property name="leaderNum" column="leaderNum"/>
        <property name="name" column="name"/>
        <property name="educationBg" column="educationBg"/>
        <property name="department" column="department"/>
        <property name="phoneNum" column="phoneNum"/>
        <property name="email" column="email"/>
        <property name="idType" column="idType"/>
        <property name="idNum" column="idNum"/>
        <property name="birthday" column="birthday"/>
    </class>
</hibernate-mapping>