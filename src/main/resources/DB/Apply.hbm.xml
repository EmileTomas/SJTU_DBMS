<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DB_model.Apply" table="apply" schema="socialpractice">
        <composite-id mapped="true" class="DB_model.ApplyPK">
            <key-property name="stuNum" column="stuNum"/>
            <key-property name="leaderStuNum" column="leaderStuNum"/>
        </composite-id>
    </class>
</hibernate-mapping>