<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DB_model.Recruit" table="recruit" schema="socialpractice">
        <id name="leaderStuNum" column="leaderStuNum"/>
        <property name="numToHire" column="numToHire"/>
        <property name="deadline" column="deadline"/>
        <property name="teamDescribe" column="teamDescribe"/>
        <property name="requirement" column="requirement"/>
    </class>
</hibernate-mapping>