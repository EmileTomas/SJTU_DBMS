<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DB_model.Grade" table="grade" schema="socialpractice">
        <id name="stuNum" column="stuNum"/>
        <property name="introClass" column="introClass"/>
        <property name="subIntroClass" column="subIntroClass"/>
        <property name="team" column="team"/>
        <property name="participation" column="participation"/>
        <property name="report" column="report"/>
    </class>
</hibernate-mapping>