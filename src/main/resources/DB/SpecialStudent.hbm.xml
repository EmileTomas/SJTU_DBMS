<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="DB_model.SpecialStudent" table="specialstudent" schema="socialpractice">
        <id name="idNum" column="idNum"/>
        <property name="name" column="name"/>
        <property name="leaderNum" column="leaderNum"/>
        <property name="school" column="school"/>
        <property name="phoneNum" column="phoneNum"/>
        <property name="email" column="email"/>
        <property name="idType" column="idType"/>
        <property name="birthday" column="birthday"/>
    </class>
</hibernate-mapping>